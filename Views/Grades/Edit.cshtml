@model project_mvc.Models.EditGradeViewModel

@{
}

<h1>Edit Course</h1>
<form method="post" action="Edit" class="mt-3">
    @Html.AntiForgeryToken()
    <div class="form-group">
        <label for="">Enrollment ID</label>
        <input type="number" class="form-control" asp-for="EnrollmentID" readonly>
    </div>
    <div class="form-group">
        <label for="">Grade</label>
        <input type="number" class="form-control" asp-for="AssignedGrade">
    </div>
    <div class="form-group">
        <label for="">Date</label>
        <input type="date" class="form-control" asp-for="Date">
    </div>
    <button type="submit" class="btn btn-primary mt-3">Submit</button>
</form>