@model List<project_mvc.Models.Domain.Instructor>

@{
    ViewData["Title"] = "Instructors";
}

<h1>Instructors</h1>

<table class="table">
    <thead>
        <tr>
            <th>ID</th>
            <th>First Name </th>
            <th>Last Name </th>
            <th>Email </th>
            <th>Faculty </th>
            <th></th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var instructor in Model)
        {
            <tr>
                <td>@instructor.InstructorID</td>
                <td>@instructor.FirstName</td>
                <td>@instructor.LastName</td>
                <td>@instructor.Email</td>
                <td>@instructor.Faculty</td>
            </tr>
        }
    </tbody>
</table>